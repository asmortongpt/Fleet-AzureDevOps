{
  "name": "fleet-websocket-emulators",
  "version": "1.0.0",
  "description": "WebSocket emulators for OBD2, Radio, and Dispatch telemetry",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "start:obd2": "node obd2-emulator.js",
    "start:radio": "node radio-emulator.js",
    "start:dispatch": "node dispatch-emulator.js",
    "start:all": "concurrently \"npm run start:obd2\" \"npm run start:radio\" \"npm run start:dispatch\"",
    "dev": "nodemon --watch . --ext js index.js"
  },
  "dependencies": {
    "ws": "^8.18.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.9"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
