{
  "name": "@fleet/security-orchestrator",
  "version": "1.0.0",
  "description": "Enterprise-grade unified vulnerability management and AI orchestration platform",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "orchestrator": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "review": "node dist/cli/index.js review",
    "finish": "node dist/cli/index.js finish",
    "dashboard": "node dist/cli/index.js dashboard",
    "finish:dashboard": "node dist/cli/index.js finish --dashboard",
    "scan": "node dist/cli/index.js scan",
    "gate": "node dist/cli/index.js gate",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@langchain/anthropic": "^0.3.13",
    "@langchain/google-genai": "^0.1.9",
    "@langchain/openai": "^0.3.17",
    "@octokit/core": "^6.1.4",
    "azure-devops-node-api": "^14.1.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "cors": "^2.8.5",
    "execa": "^9.5.2",
    "express": "^4.22.1",
    "fast-glob": "^3.3.2",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.0",
    "langchain": "^0.3.12",
    "open": "^10.2.0",
    "ora": "^8.1.1",
    "semver": "^7.6.3",
    "socket.io": "^4.8.3",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.0.3",
    "@types/semver": "^7.5.8",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "eslint": "^9.28.0",
    "typescript": "~5.7.2",
    "vitest": "^4.0.8"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "security",
    "vulnerability",
    "orchestration",
    "ai",
    "automation",
    "sast",
    "dast",
    "dependency-scanning"
