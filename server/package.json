{
  "name": "fleet-management-api",
  "version": "1.0.0",
  "description": "Fleet Management System Backend API with Azure AD SSO and 3D Model Support",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "migrate": "ts-node src/scripts/migrate.ts",
    "migrate:up": "ts-node src/scripts/migrate.ts up",
    "migrate:down": "ts-node src/scripts/migrate.ts down",
    "migrate:status": "ts-node src/scripts/migrate.ts status",
    "migrate:create": "ts-node src/scripts/migrate.ts create",
    "migrate:test": "ts-node src/scripts/migrate.ts test",
    "migrate:3d-models": "psql $DATABASE_URL -f migrations/002_vehicle_3d_models.sql",
    "seed:3d-models": "ts-node src/scripts/seed-3d-models.ts",
    "test": "jest"
  },
  "keywords": [
    "fleet",
    "management",
    "azure-ad",
    "sso",
    "3d-models",
    "sketchfab"
  ],
  "author": "Capital Tech Alliance",
  "license": "MIT",
  "dependencies": {
    "@azure/identity": "^4.0.0",
    "@azure/keyvault-secrets": "^4.8.0",
    "@azure/monitor-opentelemetry": "^1.14.2",
    "@azure/storage-blob": "^12.17.0",
    "@datadog/browser-rum": "^6.25.1",
    "@turf/turf": "^7.3.1",
    "@types/bcrypt": "^6.0.0",
    "@types/node-cron": "^3.0.11",
    "@types/node-forge": "^1.3.14",
    "applicationinsights": "^3.12.1",
    "axios": "^1.6.2",
    "bcrypt": "^6.0.0",
    "bull": "^4.16.5",
    "bullmq": "^5.66.4",
    "connect-redis": "^7.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "datadog-metrics": "^0.12.1",
    "dataloader": "^2.2.3",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.18.1",
    "heapdump": "^0.3.15",
    "helmet": "^7.1.0",
    "inversify": "^7.10.8",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.2.1",
    "node-forge": "^1.3.3",
    "pg": "^8.11.3",
    "prom-client": "^15.1.3",
    "redis": "^4.6.11",
    "reflect-metadata": "^0.2.2",
    "sharp": "^0.34.5",
    "winston": "^3.11.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/csurf": "^1.11.5",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.19.27",
    "@types/pg": "^8.10.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
