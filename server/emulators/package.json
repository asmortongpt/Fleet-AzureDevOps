{
  "name": "fleet-emulators",
  "version": "1.0.0",
  "description": "WebSocket emulators for Fleet Management System",
  "main": "index.js",
  "scripts": {
    "start:obd2": "node obd2-emulator.js",
    "start:radio": "node radio-emulator.js",
