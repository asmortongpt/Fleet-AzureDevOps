{
  "name": "fleet-management-system",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "kill": "fuser -k 5000/tcp",
    "build": "vite build",
    "build:analyze": "vite build && open dist/stats.html",
    "build:report": "vite build --mode production && echo '\nðŸ“Š Bundle analysis: dist/stats.html'",
    "build:check": "vite build && ./scripts/check-bundle-size.sh",
    "lint": "eslint .",
    "optimize": "vite optimize",
    "preview": "vite preview",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:smoke": "playwright test e2e/00-smoke-tests",
    "test:main": "playwright test e2e/01-main-modules",
    "test:management": "playwright test e2e/02-management-modules",
    "test:procurement": "playwright test e2e/03-procurement-communication-modules",
    "test:tools": "playwright test e2e/04-tools-modules",
    "test:workflows": "playwright test e2e/05-workflows",
    "test:validation": "playwright test e2e/06-form-validation",
    "test:a11y": "playwright test e2e/07-accessibility",
    "test:performance": "playwright test e2e/08-performance",
    "test:security": "playwright test e2e/09-security",
    "test:load": "playwright test e2e/10-load-testing",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:report": "playwright show-report",
    "test:all": "./scripts/run-all-tests.sh",
    "test:pa11y": "mkdir -p pa11y-screenshots && pa11y-ci",
    "test:pa11y:single": "pa11y http://localhost:5173/ --standard WCAG2AA --runner axe",
    "test:visual": "playwright test --project=visual-chromium",
    "test:visual:ui": "playwright test --project=visual-chromium --ui",
    "test:visual:headed": "playwright test --project=visual-chromium --headed",
    "test:visual:update": "UPDATE_SNAPSHOTS=true playwright test --project=visual-chromium --update-snapshots",
    "test:visual:cross-browser": "playwright test --project=visual-chromium --project=visual-firefox --project=visual-webkit",
    "test:visual:mobile": "playwright test --project=visual-mobile",
    "test:visual:report": "playwright show-report",
    "test:visual:debug": "playwright test --project=visual-chromium --debug",
    "bench": "vitest bench",
    "bench:watch": "vitest bench --watch",
    "bench:regression": "tsx benchmarks/regression-test.ts",
    "bench:report": "tsx benchmarks/utils/report-generator.ts",
    "bench:budget": "tsx benchmarks/check-budget.ts",
    "bench:all": "npm run bench && npm run bench:regression && npm run bench:report && npm run bench:budget",
    "test:load:maps": "playwright test tests/load/map-stress-test.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
