        WHEN 1 THEN 'Shell - State Street, Chicago, IL'
        WHEN 2 THEN 'Marathon - Lake Shore Dr, Chicago, IL'
        ELSE 'Speedway - Wacker Dr, Chicago, IL'
    END,
