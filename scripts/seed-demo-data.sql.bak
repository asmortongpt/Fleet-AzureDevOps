-- Generate fuel transactions for gas/diesel vehicles
-- NYC vehicles (August-November 2024)
INSERT INTO fuel_transactions (tenant_id, vehicle_id, driver_id, transaction_date, gallons, price_per_gallon,
                               odometer_reading, fuel_type, location, latitude, longitude)
SELECT
    '11111111-1111-1111-1111-111111111111',
    v.id,
    v.assigned_driver_id,
    TIMESTAMP '2024-08-01' + (random() * (TIMESTAMP '2024-11-08' - TIMESTAMP '2024-08-01')),
    CASE
        WHEN v.vehicle_type = 'sedan' THEN 12 + (random() * 3)::DECIMAL(8,3)
        WHEN v.vehicle_type = 'truck' THEN 18 + (random() * 8)::DECIMAL(8,3)
        WHEN v.vehicle_type = 'van' THEN 20 + (random() * 10)::DECIMAL(8,3)
        WHEN v.vehicle_type = 'suv' THEN 16 + (random() * 6)::DECIMAL(8,3)
    END,
    (3.45 + (random() * 0.80))::DECIMAL(6,3),
    v.odometer - (random() * 5000)::DECIMAL(10,2),
    v.fuel_type,
    CASE (random() * 4)::INT
        WHEN 0 THEN 'Shell - 42nd Street, New York, NY'
        WHEN 1 THEN 'BP - 8th Avenue, New York, NY'
        WHEN 2 THEN 'Chevron - Broadway, New York, NY'
        ELSE 'Exxon - 34th Street, New York, NY'
    END,
    40.7489 + (random() * 0.05 - 0.025),
    -73.9883 + (random() * 0.05 - 0.025)
FROM vehicles v
WHERE v.fuel_type IN ('gasoline', 'diesel')
  AND v.license_plate LIKE 'NYC-%'
  AND random() < 0.4
LIMIT 20;

