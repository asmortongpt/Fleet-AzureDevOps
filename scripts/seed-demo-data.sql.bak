        WHEN v.vehicle_type = 'suv' THEN 16 + (random() * 6)::DECIMAL(8,3)
    END,
    (3.65 + (random() * 0.75))::DECIMAL(6,3),
    v.odometer - (random() * 5000)::DECIMAL(10,2),
    v.fuel_type,
    CASE (random() * 4)::INT
