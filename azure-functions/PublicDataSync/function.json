{
  "bindings": [
    {
      "name": "myTimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 0 * * * *",
      "runOnStartup": false
    },
    {
      "name": "syncLog",
      "type": "blob",
      "direction": "out",
      "path": "public-data-sync-logs/{DateTime}.json",
      "connection": "AzureWebJobsStorage"
    }
  ],
  "scriptFile": "../dist/PublicDataSync/index.js"
}
