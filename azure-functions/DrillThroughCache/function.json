{
  "bindings": [
    {
      "name": "myTimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 */15 * * * *",
      "runOnStartup": true
    },
    {
      "name": "cacheLog",
      "type": "blob",
      "direction": "out",
      "path": "drill-through-cache-logs/{DateTime}.json",
      "connection": "AzureWebJobsStorage"
    }
  ],
  "scriptFile": "../dist/DrillThroughCache/index.js"
}
