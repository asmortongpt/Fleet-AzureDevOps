{
  "bindings": [
    {
      "name": "myTimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 0 2 * * *",
      "runOnStartup": false
    },
    {
      "name": "geocodeLog",
      "type": "blob",
      "direction": "out",
      "path": "geocoding-logs/{DateTime}.json",
      "connection": "AzureWebJobsStorage"
    }
  ],
  "scriptFile": "../dist/GeocodingService/index.js"
}
